@article{Hughes2019,
	abstract = {Over the last fifty years, gender quotas have transformed the composition of national legislatures worldwide. But a lack of systematic cross-national longitudinal data limits the questions researchers are able to ask about quotas. This article introduces a new datasetQAROT (Quota
Adoption and Reform Over Time)the first global dataset on gender quota adoption, implementation, and reform over time. Theoretically, we clarify important issues in extant quota research. The dataset moves beyond traditional categorizations of quota policies with new measures of quota
