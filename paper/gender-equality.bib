	publisher = {Palgrave Macmillan US},
	title = {Institutions---Experiences---Preferences: How Welfare State Design Affects Political Trust and Ideology},
	year = {2002},
	bdsk-url-1 = {https://doi.org/10.1007/978-0-230-10924-7_2}}

@article{Campbell2012,
	abstract = {This review examines policy feedback effects among the mass public, with a focus on social policies in the United States and Europe. It shows that existing policies feed back into the political system, shaping subsequent policy outcomes. Policies exert this effect by altering not only the capacities, interests, and beliefs of political elites and states but also those of the public. Public policies can shape political participation and attitudes. These effects can be positive or negative, enhancing or undercutting participation and conferring positive or negative messages about individuals&apos; worth as citizens. These effects originate in elements of program design, such as the size, visibility, and traceability of benefits, the proximity of beneficiaries, and modes of program administration. Thus, public policy itself shapes the distance of citizens from government, with profound implications for democratic governance.},
	author = {Campbell, Andrea Louise},
	date-added = {2025-03-23 14:38:02 -0500},
	date-modified = {2025-03-23 14:38:02 -0500},
	issn = {1545-1577},
	journal = {Annual Review of Political Science},
