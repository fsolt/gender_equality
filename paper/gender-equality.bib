@article{Hughes2019,
	abstract = {Over the last fifty years, gender quotas have transformed the composition of national legislatures worldwide. But a lack of systematic cross-national longitudinal data limits the questions researchers are able to ask about quotas. This article introduces a new datasetQAROT (Quota
