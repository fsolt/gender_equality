@article{Kuklinski1995,
	abstract = {In this essay we review recent work in public opinion in order to identify implications for the nature and study of political representation. While individuals tend to form preferences endogenously to politics, collective opinion works largely as an exogenous force on congressional policy making. We propose a conception of representation that incorporates these seemingly contradictory findings and that explicitly defines the phenomenon in dynamic terms.},
