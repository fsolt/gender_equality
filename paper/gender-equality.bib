	journal = {Political Analysis},
	number = {2},
	pages = {186-205},
	title = {{Dynamic Models for Dynamic Theories: The Ins and Outs of Lagged Dependent Variables}},
	volume = {14},
	year = {2006},
	bdsk-file-1 = {},
