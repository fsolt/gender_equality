	editor = {Lang, Sabine and Meier, Petra and Sauer, Birgit},
	pages = {73--92},
	publisher = {Springer International Publishing},
	title = {Nested Newness and Critical Junctures: Quota Implementation Flaws in the Austrian Party Democracy},
	year = {2023},
