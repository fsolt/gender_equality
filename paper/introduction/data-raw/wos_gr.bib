@article{Batalova2002,
	abstract = {The authors examine the effect of premarital cohabitation on the
   division of household labor in 22 countries. First, women do more
   routine housework than men in all countries. Second, married couples
   that cohabited before marriage have a more equal division of housework.
   Third, national cohabitation rates have equalizing effects on couples
   regardless of their own cohabitation experience. However, the influence
