@article{Glas2019a,
	abstract = {Public opinion studies argue that in Middle Eastern and North African
   countries, Muslims support gender equality less than non-Muslims. This
   overlooks the diversity in religion- feminism relations. Highly
   religious Muslims who support feminism are disregarded, even though
   in-depth studies have repeatedly pointed to their existence. Grounded in
   a structured anthology of qualitative studies on Muslim feminism, we
   provide the first ever large-scale analysis of support for Muslim
